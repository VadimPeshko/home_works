{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "no-extra-semi": "error",
    "no-plusplus": "off",
    "no-else-return": "off",
    "max-len": [
      "error",
      {
        "code": 80
      }
    ],
    "no-nested-ternary": "off",
    "no-alert": "off"
  }
}